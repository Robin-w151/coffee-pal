{
  "name": "coffee-pal",
  "version": "0.10.5",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite build && vite preview",
    "generate:schema": "yarn generate:schema:backup",
    "generate:schema:backup": "ts-json-schema-generator --path 'src/lib/models/backup.ts' --type Backup > src/assets/backup.schema.json",
    "generate:screenshots": "node scripts/generate-screenshots.js",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "sme": "source-map-explorer .svelte-kit/output/client/**/*.js",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "devDependencies": {
    "@exodus/schemasafe": "^1.3.0",
    "@floating-ui/dom": "^1.5.3",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@playwright/test": "^1.39.0",
    "@skeletonlabs/skeleton": "^2.3.0",
    "@skeletonlabs/tw-plugin": "^0.2.2",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.25.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.6",
    "@types/chance": "^1.1.4",
    "@types/file-saver": "^2.0.5",
    "@types/luxon": "^3.3.2",
    "@types/node": "^20.8.6",
    "@types/uuid": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vite-pwa/sveltekit": "^0.2.7",
    "autoprefixer": "^10.4.16",
    "chance": "^1.1.11",
    "comlink": "^4.4.1",
    "dexie": "^3.2.4",
    "dotenv-flow": "^4.0.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.34.0",
    "file-saver": "^2.0.5",
    "fuse.js": "^6.6.2",
    "husky": "^8.0.3",
    "kolorist": "^1.8.0",
    "lint-staged": "^15.0.1",
    "luxon": "^3.4.3",
    "pinst": "^3.0.0",
    "playwright": "^1.39.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "rxjs": "^7.8.1",
    "sass": "^1.69.3",
    "source-map-explorer": "^2.5.3",
    "svelte": "^4.2.1",
    "svelte-adapter-bun": "^0.5.0",
    "svelte-awesome": "^3.2.1",
    "svelte-check": "^3.5.2",
    "svelte-legos": "^0.2.2",
    "tailwindcss": "^3.3.3",
    "ts-json-schema-generator": "^1.4.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1",
    "vite": "^4.4.11",
    "vite-plugin-pwa": "^0.16.5",
    "vite-plugin-tailwind-purgecss": "^0.1.3",
    "webdav": "^5.3.0"
  },
  "type": "module",
  "packageManager": "yarn@3.6.1"
}
