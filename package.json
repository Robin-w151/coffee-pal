{
  "name": "coffee-pal",
  "version": "0.15.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite build && vite preview",
    "generate:schema": "pnpm generate:schema:backup",
    "generate:schema:backup": "ts-json-schema-generator --path 'src/lib/models/backup.ts' --type Backup > src/assets/backup.schema.json",
    "generate:screenshots": "node scripts/generate-screenshots.js",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "sme": "source-map-explorer .svelte-kit/output/client/**/*.js",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "devDependencies": {
    "@exodus/schemasafe": "^1.3.0",
    "@floating-ui/dom": "^1.6.3",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@playwright/test": "^1.43.0",
    "@skeletonlabs/skeleton": "^2.9.0",
    "@skeletonlabs/tw-plugin": "^0.3.1",
    "@sveltejs/adapter-auto": "^3.2.0",
    "@sveltejs/kit": "^2.5.5",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@types/chance": "^1.1.6",
    "@types/file-saver": "^2.0.7",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.12.4",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vite-pwa/sveltekit": "^0.4.0",
    "autoprefixer": "^10.4.19",
    "chance": "^1.1.11",
    "comlink": "^4.4.1",
    "dexie": "^4.0.1",
    "dotenv-flow": "^4.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "file-saver": "^2.0.5",
    "fuse.js": "^7.0.0",
    "husky": "^9.0.11",
    "kolorist": "^1.8.0",
    "lint-staged": "^15.2.2",
    "luxon": "^3.4.4",
    "pinst": "^3.0.0",
    "playwright": "^1.43.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "rxjs": "^7.8.1",
    "sass": "^1.74.1",
    "source-map-explorer": "^2.5.3",
    "svelte": "^4.2.12",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-awesome": "^3.3.1",
    "svelte-check": "^3.6.9",
    "svelte-legos": "^0.2.2",
    "tailwindcss": "^3.4.3",
    "ts-json-schema-generator": "^1.5.0",
    "tslib": "^2.6.2",
    "typescript": "^5.4.4",
    "uuid": "^9.0.1",
    "vite": "^5.2.8",
    "vite-plugin-pwa": "^0.19.7",
    "vite-plugin-tailwind-purgecss": "^0.2.1",
    "webdav": "^5.5.0",
    "workbox-precaching": "^7.0.0"
  },
  "type": "module"
}
